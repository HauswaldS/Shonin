{
  "name": "thermacote_server",
  "version": "1.0.0",
  "description": "Thermacote website, server side made with Express",
  "scripts": {
    "dev:server": "set NODE_ENV=development && nodemon ./server.js --exec babel-node --watch",
    "build:server": "set NODE_ENV=production && rimraf dist && babel . -d dist --ignore node_modules ",
    "start:server": "set NODE_ENV=production && node ./server.js",
    "db:migrate-up": "babel-node ./database/migrations up"
  },
  "author": "Hauswald",
  "license": "ISC",
  "dependencies": {
    "apollo-link-context": "^1.0.8",
    "async": "^2.6.1",
    "body-parser": "1.17.2",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "dotenv": "2.0.0",
    "dotenv-expand": "^0.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-jwt": "^5.3.1",
    "fs": "0.0.1-security",
    "graphql": "^0.13.2",
    "jwks-rsa": "^1.3.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "mysql2": "^1.6.1",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "uws": "^99.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.17.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2"
  }
}
